{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://designlab.local/schemas/design.generate.output.json",
    "title": "Design Generate Output",
    "type": "object",
    "properties": {
        "status": {
            "type": "string",
            "enum": [
                "ok",
                "error"
            ]
        },
        "tree": {
            "$ref": "./repl.ui.schema.json"
        },
        "errors": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
        "status"
    ],
    "additionalProperties": false
}