{
  "name": "@oods/foundry-mcp",
  "version": "0.1.0",
  "description": "Agent-first MCP interface and toolchain for the OODS Foundry design system",
  "type": "module",
  "bin": {
    "tokens-governance": "./tools/tokens-governance/index.ts"
  },
  "packageManager": "pnpm@9.12.2",
  "main": "dist/pkg/index.cjs",
  "module": "dist/pkg/index.js",
  "types": "dist/pkg/index.d.ts",
  "files": [
    "dist/pkg"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/pkg/index.d.ts",
      "import": "./dist/pkg/index.js",
      "require": "./dist/pkg/index.cjs"
    },
    "./package.json": "./package.json",
    "./provenance.json": "./dist/pkg/provenance.json"
  },
  "scripts": {
    "build": "tsc",
    "refresh:data": "node scripts/refresh-data.mjs",
    "pkg:build": "tsx scripts/pkg/build.ts",
    "pkg:version": "tsx scripts/pkg/version.ts",
    "pkg:compat": "node scripts/pkg-compat.mjs",
    "test": "vitest",
    "test:unit": "vitest run --exclude \"tests/integration/**\"",
    "test:integration": "vitest run tests/integration",
    "test:validation": "vitest run tests/validation",
    "pretest:coverage": "pnpm run build && pnpm run build:packages",
    "test:coverage": "vitest --coverage",
    "test:axe": "vitest run tests/a11y/components.a11y.test.tsx tests/a11y/dark.components.a11y.test.tsx",
    "test:contrast": "vitest run --project guardrails",
    "build:stories": "tsc --project tsconfig.stories.json",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "billing:translate": "tsx src/cli/billing-translate.ts",
    "typecheck": "tsc --noEmit --pretty false",
    "lint:enum-map": "tsx scripts/lint/enum-to-token.ts",
    "lint:audit-log": "tsx scripts/lint/audit-immutability.ts",
    "lint:brand-bleed": "tsx scripts/lint/brand-bleed.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate:types": "tsx src/generators/index.ts",
    "generate:trait-types": "tsx src/generators/index.ts",
    "generate:schema-types": "tsx scripts/types/generate.ts",
    "authz:generate-types": "tsx src/traits/authz/type-generator.ts",
    "authz:migrate": "tsx src/cli/authz-migrate.ts",
    "communication:migrate": "tsx src/cli/communication-migrate.ts",
    "authz:seed": "tsx src/cli/authz-seed.ts",
    "communication:seed:channels": "tsx src/data/communication/seed-channels.ts",
    "communication:seed:templates": "tsx src/data/communication/seed-templates.ts",
    "communication:seed:policies": "tsx src/data/communication/seed-policies.ts",
    "generate:objects": "tsx src/cli/generate-objects.ts",
    "validate": "tsx src/cli/validate.ts",
    "validate:dependencies": "tsx src/cli/validate-dependencies.ts",
    "validate:pipeline": "tsx src/cli/validate.ts --path traits --format text",
    "validate:tokens": "node tools/token-coverage/index.mjs",
    "prepare": "node tools/token-lint/setup-precommit.mjs",
    "ci:local": "npm run lint && npm run test:unit && npm run test:integration && npm run test:validation && npm run validate:pipeline && npm run tokens-validate && npm run test:contrast && npm run test:coverage",
    "lint:tokens": "node tools/token-lint/index.mjs packages/tokens/src",
    "build:tokens": "node packages/tokens/scripts/build.mjs",
    "check:tokens": "node packages/tokens/scripts/build.mjs --check",
    "tokens:transform": "tsx scripts/tokens/transform.ts",
    "tokens:validate": "tsx scripts/tokens/transform.ts --check",
    "tokens:lint-semantic": "tsx scripts/tokens/lint-semantic.ts",
    "tokens:guardrails": "tsx scripts/tokens/color-guardrails.ts",
    "tokens:validate-viz": "tsx scripts/tokens/validate-viz-scales.ts",
    "tokens-validate": "pnpm run tokens:validate && pnpm run tokens:lint-semantic && pnpm run tokens:guardrails -- --quiet && pnpm run tokens:validate-viz",
    "tokens:governance": "tsx tools/tokens-governance/index.ts",
    "build:packages": "pnpm --filter @oods/* run build",
    "prea11y:check": "pnpm --filter @oods/tokens run build",
    "a11y:check": "node tools/a11y/index.mjs check",
    "prea11y:diff": "pnpm --filter @oods/tokens run build",
    "a11y:diff": "node tools/a11y/index.mjs diff",
    "prea11y:baseline": "pnpm --filter @oods/tokens run build",
    "a11y:baseline": "node tools/a11y/index.mjs baseline",
    "overlays:proof": "node scripts/overlays-proof.mjs",
    "compliance:proof": "tsx scripts/compliance/proof.ts",
    "tenancy:seed": "tsx scripts/tenancy/seed.ts",
    "tenancy:test": "vitest run tests/tenancy/",
    "tenancy:check": "tsx scripts/tenancy/check.ts",
    "adoption:audit": "node scripts/adoption/audit.mjs",
    "adoption:storybook-checklist": "node scripts/adoption/storybook-checklist.mjs",
    "usage:sample": "node scripts/usage/emit-sample.mjs",
    "usage:aggregate": "tsx scripts/usage/aggregate.ts",
    "user:prefs": "tsx scripts/preferences/preference-cli.ts",
    "vrt:fallback": "playwright test --config testkits/vrt/playwright.config.ts",
    "vrt:fallback:update": "playwright test --config testkits/vrt/playwright.config.ts --update-snapshots",
    "vrt:layouts": "playwright test testkits/vrt/stories/viz/layout.visual.spec.ts --config testkits/vrt/playwright.config.ts --project=chromium",
    "vrt:lightdark": "node testkits/vrt/storycap.lightdark.mjs --out artifacts/vrt/lightdark/local",
    "vrt:hc": "node testkits/vrt/storycap.hc.mjs --out artifacts/vrt/hc/local",
    "perf:harness": "tsx testkits/perf-harness/cli.ts",
    "perf:harness:ci": "pnpm run build-storybook && pnpm dlx serve storybook-static -l 6006 & sleep 5 && pnpm perf:harness",
    "perf:baseline": "node scripts/perf/calc-baseline.mjs",
    "perf:check": "node scripts/perf/check-regressions.mjs",
    "perf:notify": "node scripts/perf/notify-slack.mjs",
    "validate:diagnostics": "node scripts/validate-diagnostics-schema.mjs",
    "registry:list": "tsx src/cli/registry-list.ts",
    "viz:suggest": "node scripts/viz/suggest-chart.mjs",
    "viz:best-practices": "node scripts/viz/best-practices.mjs",
    "viz:figma-handshake": "tsx scripts/viz/export-figma-handshake.ts",
    "viz:scaffold-spatial": "tsx src/cli/viz-scaffold-spatial.ts",
    "object:list": "tsx src/cli/object-commands.ts list",
    "object:resolve": "tsx src/cli/object-commands.ts resolve",
    "object:compose": "tsx src/cli/object-commands.ts compose",
    "object:validate": "tsx src/cli/object-commands.ts validate",
    "object:create": "tsx src/cli/object-commands.ts create",
    "object:generate": "tsx src/cli/object-commands.ts generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic:dry-run": "scripts/chromatic-run.sh --dry-run --storybook-build-dir storybook-static --exit-zero-on-changes --auto-accept-changes main",
    "chromatic:publish": "scripts/chromatic-run.sh --storybook-build-dir storybook-static --auto-accept-changes main",
    "sb:verify": "node scripts/sb-verify.mjs",
    "local:pr-check": "node scripts/local-pr-check.mjs",
    "agent:overlays": "node scripts/agent-overlay-recipe.mjs",
    "current-state:sprint06": "node ../scripts/diagnostics/sprint06-current-state.mjs",
    "current-state:sprint07": "node ../scripts/diagnostics/sprint07-current-state.mjs",
    "current-state:sprint08": "node ../scripts/diagnostics/sprint08-current-state.mjs",
    "chromatic": "scripts/chromatic-run.sh --storybook-build-dir storybook-static --auto-accept-changes main",
    "normalize:contexts": "node scripts/normalize-contexts.cjs",
    "validate:contexts": "node scripts/validate-contexts.cjs",
    "pipeline:push": "pnpm run build:tokens && pnpm run build-storybook && (pnpm run chromatic || echo chromatic skipped) && pnpm run vrt:layouts && pnpm run vrt:hc && pnpm run current-state:sprint08 && pnpm run validate:contexts && pnpm run purity:audit",
    "pack:verify": "bash scripts/release/local-release.sh",
    "release:dry-run": "node scripts/release/dry-run.mjs",
    "purity:audit": "node scripts/purity/audit.js",
    "state:token-breaking": "CMOS_TOKEN_LABELS=token-change:breaking node scripts/state-assessment.mjs",
    "bridge": "pnpm --filter @oods/mcp-bridge start",
    "bridge:dev": "pnpm --filter @oods/mcp-bridge dev"
  },
  "keywords": [
    "design-system",
    "mcp",
    "model-context-protocol",
    "design-tokens",
    "dtcg",
    "agent",
    "ai",
    "traits",
    "composition",
    "typescript"
  ],
  "author": "Design System Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/kneelinghorse/OODS-Foundry-mcp.git"
  },
  "homepage": "https://github.com/kneelinghorse/OODS-Foundry-mcp#readme",
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@oods/a11y-tools": "workspace:*",
    "@oods/tokens": "workspace:*",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/mdx2-csf": "1.1.0",
    "@storybook/react": "9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@tokens-studio/sd-transforms": "^1.3.0",
    "@types/d3-geo": "^3.1.0",
    "@types/d3-scale": "^4.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/react-window": "^1.8.6",
    "@types/semver": "^7.5.8",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "axe-core": "^4.10.0",
    "colorjs.io": "^0.5.2",
    "eslint": "^8.56.0",
    "eslint-plugin-storybook": "^9.1.10",
    "jsdom": "^24.0.0",
    "json-schema-to-typescript": "^13.1.1",
    "markdown-to-jsx": "^7.7.15",
    "pg-mem": "^3.0.5",
    "playwright": "^1.56.1",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.7",
    "semver": "^7.6.3",
    "sql.js": "^1.13.0",
    "storybook": "^9.1.10",
    "style-dictionary": "^4.4.0",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.2.4",
    "tsx": "^4.20.6",
    "typescript": "~5.3.3",
    "vega-lite": "^6.4.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "@rjsf/core": "^5.17.1",
    "@rjsf/utils": "^5.17.1",
    "@rjsf/validator-ajv8": "^5.17.1",
    "@types/geojson": "^7946.0.16",
    "@types/topojson-client": "^3.1.5",
    "@types/topojson-specification": "^1.0.5",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "d3-geo": "^3.1.1",
    "d3-scale": "^4.0.2",
    "echarts": "^6.0.0",
    "js-yaml": "^4.1.0",
    "luxon": "^3.7.2",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-window": "^1.8.11",
    "topojson-client": "^3.1.0",
    "vega": "^6.2.0",
    "vega-embed": "^7.1.0",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@oods/tokens": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@oods/tokens": {
      "optional": false
    }
  }
}
